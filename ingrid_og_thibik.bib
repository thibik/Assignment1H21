  shorttitle = {Towards {{FAIR}} Protocols and Workflows},
  author = {Celebi, Remzi and Moreira, Joao Rebelo and Hassan, Ahmed A. and Ayyar, Sandeep and Ridder, Lars and Kuhn, Tobias and Dumontier, Michel},
